{
  "name": "magnetstreamer",
  "version": "1.0.0",
  "description": "SaaS application to stream torrent videos directly in browser - Built by ProbotisOP",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "torrent",
    "streaming",
    "video",
    "webtorrent",
    "saas"
  ],
  "author": "ProbotisOP (https://github.com/ProbotisOP)",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "webtorrent": "^1.7.0",
    "socket.io": "^4.6.1",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
